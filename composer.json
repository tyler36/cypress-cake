{
    "name": "tyler36/cypress-cake",
    "description": "Cypress plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "^5.0.1",
        "vierge-noire/cakephp-fixture-factories": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Tyler36\\CypressCake\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tyler36\\CypressCake\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:tyler36/cypress-support.git"
        }
    ]
}
