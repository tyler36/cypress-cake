{
    "name": "tyler36/cypress-cake",
    "description": "Cypress plugin for CakePHP",
    "type": "cakephp-plugin",
    "version": "1.0",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "^5.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Tyler36\\Cypress\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tyler36\\Cypress\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
